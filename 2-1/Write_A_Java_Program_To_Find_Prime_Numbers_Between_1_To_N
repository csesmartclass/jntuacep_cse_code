
 //Prime number between 1 and n.
	import java.io.*;
	import java.util.*;
	public class Prime 
	{

		public static void main(String[] args) 
		{
			int n,s1=0;
			Scanner s=new Scanner(System.in);
			System.out.println("Enter a number to check whether it is a prime or not: ");
			n=s.nextInt();
			for(int i=2;i<n;i++)
			{
				if(n%i==0)
				{
					s1++;
				}
			}
		
			if(s1>0)
			{
				System.out.println("Emtered number "+n+" is not a prime number.");
			}
			else
				System.out.println("Emtered number "+n+" is a prime number.");
			s.close();
		}
	
	}

